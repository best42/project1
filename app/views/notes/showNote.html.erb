<div class="container">
  <div class="center">
      <p><%= note.title %></p>
      <p><%= note.title %></p>
      <p><%= note.description %></p>
      <p><%= note.subject %></p>
      <p><%= note.teacher %></p>
      <p><%= note.rating %></p>
      <p><%= note.owner %></p>
      <p><%= note.create_note %></p>
      <%= image_tag note.image.url %>

      <p><%= link_to 'Show', note %></p>
      <p><%= link_to 'Edit', edit_note_path(note) %></p>
      <p><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></p>
  </div>
</div>

